{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "start": {
      "dependsOn": ["migrate", "build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "./dist"]
    },
    "lint:fix": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "cache": false,
      "persistent": true
    },
    "migrate": {
      "cache": false
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
